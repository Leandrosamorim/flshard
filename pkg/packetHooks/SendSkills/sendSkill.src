const TYPE := 3;

program sendSkill()
  Print( "Hooking Send Skill..." );
  return 1;
endprogram

exported function HandleSendSkillSent( who, byref packet )
  who := who;
  var tipo := packet.GetInt8(TYPE);
  if(!tipo)
    return 0;
  endif
  //print("SendSKILL");
  return 1;
endfunction

exported function HandleSendSkillReceived( who, byref packet )
  who := who;
  packet := packet;
  //print("ReceiveSKILL");
  return 1;
endfunction
